<!DOCTYPE html>
<html>
<head>
  <title>Logs - RAPIDWIFI-ZONE</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <header>
    <h1>System Logs</h1>
    <nav>
      <a href="/admin">Admin Dashboard</a>
      <a href="/analytics">Analytics</a>
      <a href="/logout">Logout</a>
    </nav>
  </header>

  <main>
    <h2>Audit & Download Logs</h2>
    <div class="logs-container">
      <table class="logs-table">
        <thead>
          <tr>
            <th>ID</th>
            <th>Action</th>
            <th>User</th>
            <th>Profile</th>
            <th>Filename / Details</th>
            <th>Channel</th>
            <th>Status</th>
            <th>Timestamp</th>
          </tr>
        </thead>
        <tbody>
          <% logs.forEach(function(log) { %>
            <tr>
              <td data-label="ID"><%= log.id %></td>
              <td data-label="Action"><%= log.action %></td>
              <td data-label="User"><%= log.user || log.username %></td>
              <td data-label="Profile"><%= log.profile || '-' %></td>
              <td data-label="Filename / Details"><%= log.filename || log.details || '-' %></td>
              <td data-label="Channel"><%= log.channel || '-' %></td>
              <td data-label="Status"><%= log.status || '-' %></td>
              <td data-label="Timestamp"><%= log.timestamp %></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </main>

  <footer>
    <p>&copy; 2026 RAPIDWIFI-ZONE</p>
  </footer>
</body>
</html>

